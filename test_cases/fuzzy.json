{
  "name": "Mixed tests",
  "description": "Test cases to test all types of searches, including fuzzy matches",
  "priorityThresh": 10,
  "distanceThresh": 300,
  "normalizers": {
    "name": [
      "toUpperCase",
      "removeNumbers"
    ]
  },
  "tests": [
    {
      "id": 0,
      "status": "pass",
      "user": "hsldevcom",
      "info": "POI search",
      "type": "fuzzy",
      "in": {
        "text": "Eduskuntatalo, Helsinki"
      },
      "expected": {
        "properties": [
          { "name": "Eduskuntatalo"
          }
        ]
      }
    },
    {
      "id": 1,
      "status": "pass",
      "user": "hsldevcom",
      "info": "address search",
      "type": "fuzzy",
      "in": {
        "text": "Ruoholahdenkatu 8, Helsinki"
       },
       "expected": {
        "properties": [
          { "name": "Ruoholahdenkatu 8"
          }
        ]
      }
    },
    {
      "id": 2,
      "status": "pass",
      "user": "hsldevcom",
      "info": "POI search",
      "type": "fuzzy",
      "in": {
        "text": "Pikku Huopalahti"
       },
       "expected": {
        "properties": [
          { "name": "Pikku Huopalahti"
          }
        ]
      }
    },
    {
      "id": 4,
      "status": "pass",
      "user": "hsldevcom",
      "info": "Fuzzy street search",
      "type": "fuzzy",
      "in": {
        "text": "Manerheimintie"
       },
       "expected": {
        "properties": [
          { "name": "Mannerheimintie"
          }
        ]
      }
    },
    {
      "id": 5,
      "status": "pass",
      "user": "hsldevcom",
      "info": "Fuzzy address search",
      "type": "fuzzy",
      "in": {
        "text": "Appikatu 3, Helsinki"
       },
       "expected": {
        "properties": [
          { "name": "Alppikatu 3",
	    "localadmin": "Helsinki"
          }
        ]
      }
    },
    {
      "id": 6,
      "status": "pass",
      "user": "hsldevcom",
      "info": "Really hard fuzzy POI search",
      "type": "fuzzy",
      "in": {
        "text": "Turtolan Citymarket, Tampere"
       },
       "expected": {
        "properties": [
